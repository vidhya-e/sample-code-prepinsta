import java.util.*;
class Greatest_Of_Two_Numbers {
    public static void main(String[] args) {
         int num1,num2;
         System.out.println("Enter 2 numbers: ");
        Scanner sc=new Scanner (System.in);              // Using scanner class to get the input from user and store it in "num" variable
        num1=sc.nextInt();
        num2=sc.nextInt();
        
        if(num1==num2)
        System.out.println("Both number are equal ");
        else
        System.out.println(Math.max(num1,num2)+ " is the greater number");     // using a inbuilt math function "max" to find the maximum of 2 numbers
        }
    }
